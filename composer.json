{
    "name": "specialfield/button-link",
    "description": "A Laravel Nova button-link field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Specialfield\\ButtonLink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Specialfield\\ButtonLink\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
